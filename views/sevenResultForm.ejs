              <li><a href="/contact" class="contact flex justify-center text-[30px] font-Aclonica text-black border-b border-gray-400 py-5 transition-all hover:bg-black hover:text-white">
                  <span class="link_contact">C</span>
                  <span class="link_contact">O</span>
                  <span class="link_contact">N</span>
                  <span class="link_contact">T</span>
                  <span class="link_contact">A</span>
                  <span class="link_contact">C</span>
                  <span class="link_contact">T</span>
                </a></li>
