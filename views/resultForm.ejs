        <!--====== SM_HEADER =====-->
        
        <header class="small_header w-full bg-red-500 relative sm:hidden overflow-hidden">
          <div class="header_head fixed bg-[#DEFFEE69] backdrop-blur-sm top-0 left-0 w-full h-20 flex justify-between items-center px-5 shadow-lg z-[109]">

            <div class="sm_logo font-bold text-2xl z-[999] overflow-hidden">
              <h1 class="text-4xl flex tracking-tight font-Baumans px-1">
                <span>B</span>
                <span>P</span>
                <span>M</span>
                <span>H</span>
                <span>S</span>
              </h1>
            </div>

            <div class="relative">
              <button id="menuToggle" class="w-10 h-10 flex items-center justify-center overflow-hidden text-4xl text-gray-700 rounded-full absolute left-[35px] top-[-20px]">
                <i class="bx bx-user-circle user_i absolute flex items-center justify-center">
                  <!--  <img src="" class="w-full h-full object-cover absolute flex items-center justify-cente rounded-full">-->
                </i>
              </button>

              <div id="menuDropdown" class="hidden absolute right-[-100px] mt-10 w-48 bg-[#DEFFEE] text-gray-800 font-cy border border-zinc-100 rounded-lg shadow-lg">
                <ul class="py-1">
                  <li><a href="/student/profile" class="flex items-center px-4 py-2 text-sm hover:bg-zinc-900 hover:text-white"><i class="bx bx-user mr-2"></i> Profile</a></li>
                  <li><a href="/student/settings" class="flex items-center px-4 py-2 text-sm hover:bg-zinc-900 hover:text-white"><i class="bx bx-cog mr-2"></i> Settings</a></li>
                  <li><a href="user/logout" class="flex items-center px-4 py-2 text-sm hover:bg-zinc-900 hover:text-white"><i class="bx bx-log-out mr-2"></i> Logout</a></li>
